{
  "name": "web-playground-sync",
  "displayName": "web-playground-sync",
  "description": "no",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.78.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "web-playground-sync.fetch",
        "title": "Fetch web playground URL"
      },{
        "command": "web-playground-sync.generate",
        "title": "Generate web playground URL"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "build": "npm run compile",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "20.2.3",
    "@types/vscode": "^1.78.0",
    "@typescript-eslint/eslint-plugin": "^5.59.7",
    "@typescript-eslint/parser": "^5.59.7",
    "@vscode/test-electron": "^2.3.2",
    "eslint": "^8.41.0",
    "glob": "^10.2.6",
    "mocha": "^10.2.0",
    "ts-loader": "^9.4.3",
    "typescript": "^5.0.4",
    "webpack": "^5.84.0",
    "webpack-cli": "^5.1.1"
  },
  "dependencies": {
    "lz-string": "^1.5.0"
  }
}